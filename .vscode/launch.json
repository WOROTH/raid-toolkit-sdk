{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "compounds": [],
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "justMyCode": false,
            "console": "integratedTerminal"
        },
        {
            "name": "Launch Prebuild",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SDK/Model.PreBuild/bin/Debug/net5.0/Model.PreBuild.exe",
            "args": [
                "${workspaceFolder}\\bin"
            ],
            "env": {
                "FORCE": "true",
                "RTK_DEBUG": "true",
                "app:MemoryLogging": "Error"
            },
            "justMyCode": false,
            "cwd": "${workspaceFolder}/SDK/Model.PreBuild",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Service",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SDK/Service/bin/Debug/net5.0-windows/win-x64/Raid.Service.exe",
            "args": [],
            "env": {
                "RTK_DEBUG": "true",
                "app:MemoryLogging": "Error"
            },
            "justMyCode": false,
            "cwd": "${workspaceFolder}/SDK/Service",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Service (Release)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build release",
            "program": "${workspaceFolder}/SDK/Service/bin/Release/net5.0-windows/win-x64/Raid.Service.exe",
            "args": [],
            "env": {
                "RTK_DEBUG": "true",
                "app:MemoryLogging": "Error"
            },
            "justMyCode": false,
            "cwd": "${workspaceFolder}/SDK/Service",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Service (Protocol)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SDK/Service/bin/Debug/net5.0-windows/win-x64/Raid.Service.exe",
            "args": [
                "open",
                "rtk://open?channel=CNTvMrQnRF3li0hGX2Apj&origin=chrome-extension://omalebghpgejjiaoknljcfmglgbpocdp"
            ],
            "env": {
                "RTK_DEBUG": "true"
            },
            "cwd": "${workspaceFolder}/SDK/Service",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Register Service",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SDK/Service/bin/Debug/net5.0-windows/win-x64/Raid.Service.exe",
            "args": [
                "open"
            ],
            "env": {
                "RTK_DEBUG": "true"
            },
            "cwd": "${workspaceFolder}/SDK/Service",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}